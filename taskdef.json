  "executionRoleArn": "<EXECUTION_ROLE_ARN>",
  "cpu": "256",
  "memory": "512",
  "requiresCompatibilities": ["FARGATE"],
  "containerDefinitions": [
    {
      "name": "<CONTAINER_NAME>",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ]
}
